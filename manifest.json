{
  "manifest_version": 3,
  "name": "Pin Tabs",
  "version": "1.0",
  "description": "Open bookmark folders with 'PM-' prefix in new windows and pin the tabs automatically.",
  "permissions": ["bookmarks", "tabs", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
